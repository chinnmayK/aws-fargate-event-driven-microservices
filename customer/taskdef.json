{
    "executionRoleArn": "arn:aws:iam::202533520289:role/ecs_task_execution_role",
    "containerDefinitions": [
        {
            "name": "customer",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8001,
                    "hostPort": 8001,
                    "protocol": "tcp"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/microservices",
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "customer-task"
}